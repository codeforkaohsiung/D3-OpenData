<div class="row mbl">
  <div class="col-sm-6" >
    <img id="chartImg" src="/upload/test-img.png" alt="" class="img-responsive">
    <canvas id="canvas" style="display:none" width="1000" height="531"></canvas>
    <div class="text-center mtm">
      <a id="updateImg" type="button" class="btn btn-default btn-xs" ng-click="updateImage()">更新圖片</a>
      <a id="downloadImg" type="button" class="btn btn-default btn-xs">下載圖片</a>
    </div>
  </div>
  <div class="col-sm-6">
    <form action="" class="form">
      <div class="form-group">
        <label for="chartName">圖表名稱</label>
        <input type="text" class="form-control" id="chartName" placeholder="圖表名稱" ng-model="appModel.content.chartName">
      </div>
      <div class="form-group">
        <label for="chartKey">Google 試算表 Key </label>
        <input type="text" class="form-control" ng-model="appModel.chartShkey" disabled="">
      </div>
      <div class="form-group">
        <label for="chartDiscription">描述</label>
        <textarea class="form-control" id="chartDiscription" rows="3" placeholder="對你的圖表做點描述" ng-model="appModel.content.description"></textarea>
      </div>
      <div class="text-right">
        <button type="button" class="btn btn-primary jq-SaveData">分享到展示區</button>
      </div>
    </form>
  </div>
</div>	